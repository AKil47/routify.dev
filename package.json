{
  "name": "v2.routify.dev",
  "private": true,
  "license": "MIT",
  "scripts": {
    "dev": "run-p dev:*",
    "dev:routify": "routify",
    "dev:vite": "vite",
    "build": "run-s build:*",
    "build:routify": "routify -b",
    "build:vite": "vite build",
    "build:static": "spank",
    "build:search": "poindexter",
    "assets": "pwa-asset-generator public/logo-square.svg public/icons/pwa --background \"#FFFFFF\" --padding \"16px\""
  },
  "devDependencies": {
    "@roxi/routify": "^2.15.2",
    "@sveltech/bricks": "^1.0.0",
    "@sveltejs/vite-plugin-svelte": "^1.0.0-next.44",
    "date-fns": "^2.14.0",
    "marked": "^1.1.0",
    "mdsvex": "^0.8.9",
    "npm-run-all": "^4.1.5",
    "poindexter": "^1.0.3",
    "prism-svelte": "^0.4.6",
    "prismjs": "^1.20.0",
    "pwa-asset-generator": "^6.0.8",
    "remark-slug": "^6.0.0",
    "rollup": "^2.17.1",
    "routify-plugin-frontmatter": "^1.0.1",
    "sass": "^1.52.1",
    "spank": "^1.4.0",
    "spassr": "^1.1.2",
    "svelte": "^3.29.7",
    "svelte-preprocess": "^4.10.6",
    "svelte-prism": "^1.1.3",
    "vite": "^2.9.9",
    "vite-plugin-pwa": "^0.12.0"
  },
  "routify": {
    "extensions": "svelte,html,svx,md",
    "dynamicImports": true,
    "plugins": {
      "routify-plugin-frontmatter": {}
    }
  },
  "configent": {
    "template": "routify-starter:1.0.0"
  },
  "poindexter": {
    "output": "dist/poindexter.bundle.json",
    "ignoreSelectors": [
      "nav",
      ".c-pagination"
    ],
    "contentSelectors": [
      ".c-sidebar-layout__main",
      "main"
    ]
  }
}
